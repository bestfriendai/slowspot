{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_ENV": "preview"
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "autoIncrement": "buildNumber"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "autoIncrement": "versionCode"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "autoIncrement": "buildNumber"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": "versionCode"
      },
      "cache": {
        "disabled": false,
        "paths": ["node_modules"]
      }
    },
    "production-simulator": {
      "extends": "production",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Release"
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "leszekszpunar@gmail.com",
        "appleTeamId": "QQHRMJ6B78"
      }
    },
    "production": {
      "ios": {
        "appleId": "leszekszpunar@gmail.com",
        "appleTeamId": "QQHRMJ6B78",
        "ascApiKeyPath": "./AuthKey_W3TGVKY5Z6.p8",
        "ascApiKeyIssuerId": "7fbde26f-7ea0-4e56-8d1e-23174a411710",
        "ascApiKeyId": "W3TGVKY5Z6",
        "sku": "com.iteon.slowspot"
      },
      "android": {
        "serviceAccountKeyPath": "./android-service-account.json",
        "track": "production",
        "releaseStatus": "completed",
        "changesNotSentForReview": false
      }
    }
  }
}
